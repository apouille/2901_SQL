
#/ Crée la table COURSES
CREATE TABLE `courses` (
`id` INTEGER PRIMARY KEY AUTOINCREMENT, `name` TEXT
`titre` TEXT,
`description` TEXT
);


#/ Crée la table LESSONS
CREATE TABLE `lessons` (
`id` INTEGER PRIMARY KEY AUTOINCREMENT, `name` TEXT,
`id_courses` TEXT,
`titre` TEXT,
`description` TEXT
);


